sealed class Vehicle{
    fun getClass(base: Vehicle){
    	println("This is called from object of ${base.javaClass}")}
}

class Car: Vehicle(){
    
}

class Scooter: Vehicle(){
    
}

class Bicycle: Vehicle(){
    
}

fun main(args: Array<String>){
    var vehicle = Vehicle()
    var car = Car()
    var scooter = Scooter()
    var bicycle = Bicycle()
    
    car.getClass(car);
    scooter.getClass(scooter);
    bicycle.getClass(bicycle);
    
    
    
}

sealed class Vehicle{
    
    var vehicletype = "Any"
   
    open fun type(vehicletype){
        println(vehicletype)
    }
}

class Car:Vehicle{
     fun type(vehicletype){
        println("car")
    }
}

class Scooter:Vehicle{
      fun type(vehicletype){
        println("scooter")
    }
}

class Bicycle:Vehicle{
      fun type(vehicletype){
        println("bicycle")
    }
}

fun main(args:Array<String){
    println("I am in Main function")
    
    val c = Car()
    val s = Scooter()
    val b = Bicycle()
}
